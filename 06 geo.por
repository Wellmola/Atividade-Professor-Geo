programa{
funcao inicio(){

    cadeia nomeProd
    real valorProd
    caracter op = 'n'


    faca{
        escreva("Insira o nome do produto: ")
        leia(nomeProd)
        escreva("Insira o valor do produto: ")
        leia(valorProd)

        escreva("Deseja continuar? 's' para sim, 'n' para não ")
        leia(op)
    }enquanto(op != 'n')
  



    }
}
